    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.40",
    "svelte": "^4.2.7",
    "svelte-check": "^3.6.0",
    "tailwindcss": "^3.4.6",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^5.0.3",
    "wrangler": "^3.66.0"
  },
  "type": "module",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.6.0",
    "@fortawesome/free-brands-svg-icons": "^6.6.0",
